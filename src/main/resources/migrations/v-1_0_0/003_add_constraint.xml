<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="add foreign key" author="Ilya">
        <addForeignKeyConstraint baseTableSchemaName="app"
                                 baseTableName="device"
                                 baseColumnNames="home_id"
                                 constraintName="fk_device_home_id"
                                 referencedTableSchemaName="app"
                                 referencedTableName="home"
                                 referencedColumnNames="id"
                                 onDelete="CASCADE"
                                 onUpdate="CASCADE"/>
    </changeSet>

</databaseChangeLog>